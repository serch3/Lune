{
    "manifest_version": 3,
    "name": "Lune",
    "version": "1.0",
    "description": "Aesthethic start page, no ads, no tracking, no news.",
    "icons": {
        "16": "icon-16.png",
        "48": "icon-48.png",
        "128": "icon-128.png"
    },
    "chrome_url_overrides": {
        "newtab": "index.html"
    },
    "permissions": ["storage", "topSites"],
    "host_permissions": [
        "https://suggestqueries.google.com/",
        "https://ac.duckduckgo.com/",
        "https://api.bing.com/",
        "https://yandex.com/"
    ]
}