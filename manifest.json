{
    "manifest_version": 3,
    "name": "Lune",
    "version": "1.0",
    "description": "Minimalistic start page, no ads, no tracking, no news.",
    "chrome_url_overrides": {
        "newtab": "index.html"
    },
    "permissions": ["storage", "topSites"],
    "host_permissions": [
        "https://suggestqueries.google.com/",
        "https://ac.duckduckgo.com/",
        "https://api.bing.com/",
        "https://yandex.com/"
    ],
    "action": {
        "default_popup": "index.html"
    }
}